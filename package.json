  "scripts": {
    "firefox": "npm run watch & web-ext run --source-dir=dist/",
    "build": "rollup -c rollup.config.js",
    "watch": "npm run build -- -w",
    "test": "mocha \"src/**/*.spec.js\" --require babel-register --require babel-polyfill --require testSetup.js",
