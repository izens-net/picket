    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "rollup": "^1.0.0",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-replace": "^2.1.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
